<!doctype html>
<html lang='en' class="width-100 height-100">
<head>
  {{> head }}
  <meta property="fb:app_id" content="187816851587993">
  <meta property="og:url" content="/">
  <meta property="og:type" content="article">
  <meta property="og:title" content="Location data for Buncombe County, NC">
  <meta property="og:description" content="The Climate Explorer allows you to view historical and projected climate variables and assess the impacts of climate change on the things you care about">
  <meta property="og:image" content="/resources/img/og.jpg">
</head>

<body class="width-100 height-100">

  {{> secondary_header }}

    <div id="local-climate-charts-viewport" class="padding-horrizontal d-flex d-flex-column">

      <div id="search-row" class="padding-horizontal d-flex-row flex-justify padding-top padding-bottom-half d-flex-justify" >

        <div class="rounded-box-secondary input-outer padding-horrizontal bottom-padding default-btn-height search-box-secondary width-40">
          <input tabindex="0" id="cards-search-input" type="text" class="location-mapper input-inner-default border-none default-btn-height" autocomplete="off" placeholder="Enter county, city, or zip code">
          <span class="icon icon-search search-default float-right padding-default"></span>
          <div id="clear-location-wrapper" class="">
            <a id="clear-location-link"  href="../local-climate-charts" class="d-none"></a>
            <span id="clear-location" nav="local-climate-charts" link="clear-location-link" class="fas fa-times-circle"></span>
          </div>
        </div>

        <div id="stations-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center disabled">
          <div class="select">
            <div tabindex="1" id="stations-select-vis" class="select-styled disabled" rel="none">Stations</div>
            <ul class="select-options" style="display: none;">
              <li rel="none" class="default-select-option">Stations</li>
              <li rel="station-1" class="default-select-option">Stations 1</li>
              <li rel="station-2" class="default-select-option">Stations 2</li>
              <li rel="station-3" class="default-select-option">Stations 3</li>
              <li rel="station-4" class="default-select-option">Stations 4</li>
            </ul>
          </div>
        </div>

        <div id="downloads-select-wrapper" class="rounded-choice-box padding-horrizontal default-btn-height top-select-wrapper d-flex-center">

          <div class="select download-select">
            <div tabindex="2" id="downnloads-select-vis" class="select-styled" rel="none" href="javascript:void(0);">
              <i class="fas fa-download select-icon"></i>
              <a rel="none" class="download-link" href="javascript:void(0);" >Downloads</a>
            </div>
            <ul class="select-options" style="display: none;">
              <li id="download-image" rel="download-image" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
                <a tabindex="3" rel="download-image" class="download-link" href="javascript:void(0);" >Download image</a>
              </li>
              <li id="download-observed-data" rel="download-observed-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
                <a tabindex="4" rel="download-observed-data" class="download-link" href="javascript:void(0);" >Download Observed Data</a>
              </li>
              <li id="download-historical-modeled-data" rel="download-historical-modeled-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
                <a tabindex="5" rel="download-historical-modeled-data" class="download-link" href="javascript:void(0);" >Download Historical Modeled Data</a>
              </li>
              <li id="download-projected-modeled-data" rel="download-projected-modeled-data" icon="fas fa-download select-icon" href="javascript:void(0);" class="default-select-option">
                <a tabindex="6" rel="download-projected-modeled-data" class="download-link" href="javascript:void(0);" >Projected Modeled Data</a>
              </li>
              <li id="download-interpreting" rel="download-interpreting" icon="fas fa-download select-icon" class="default-select-option">
                <a tabindex="7" rel="download-interpreting" class="download-link" href="/resources/data/Key-to-Climate-Explorer-Download-Filenames-and-Column-Headings.xlsx" >Information for Interpreting Data</a>
              </li>
            </ul>
          </div>
        </div>

      </div>

      <div id="info-row" class="padding-top width-100" >
        <div class="rounded-filters-box padding-default width-100">

          <div class="filter-border-bottom d-flex-row">

            <div id="info-text-wrapper" class="width-90">
              <i class="fas fa-chart-line icon-info-box"></i>
              <span id="default-city-state" class="text-info-box" ></span>
              <span id="default-in" class="text-info-box" > in </span>
              <span id="default-city-county" class="text-info-box"></span>
              <span id="default-dash" class="text-info-box" > - </span>
              <span id="default-chart-map-varriable" class="text-info-box" rel="tmax">Avg Daily Max Temp (Â°F)</span>
            </div>

            <div id="filters-toggle" class="width-10">
              <div class="select filters-toggle-select">
                <div id="filters-toggle-select-vis" class="select-styled" rel="filter"></div>
              </div>
            </div>

          </div>

          <div id="filters-row" class="d-flex-row flex-justify padding-top  d-flex-left" >

            <div id="varriable-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-30">
              {{> variable-selector }}
            </div>


            <a id="chartmap-btn-chart-link"  href="../local-climate-charts" class="d-none"></a>
            <a id="chartmap-btn-map-link"  href="../local-climate-maps" class="d-none">Map</a>
            <div id="chartmap-wrapper" class="padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div tabindex="100" id="btn-chart" class="btn-chart btn-default-selected circle-box-left d-flex-center width-50" val="chart" nav="local-climate-charts" link="chartmap-btn-chart-link" sel="chartmap-select-vis">
                Chart
              </div>
              <div tabindex="101" id="btn-chart" class="btn-map btn-default circle-box-right d-flex-center width-50"  nav="local-climate-maps"  val="map" link="chartmap-btn-map-link" sel="chartmap-select-vis">
                Map
              </div>
            </div>

            <div id="chartmap-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div class="select chartmap-select">
                <div tabindex="102" id="chartmap-select-vis" class="select-styled" rel="chart">Chart</div>
                <ul class="select-options">
                  <li tabindex="103" id="chartmap-select-chart-link" rel="chart" class="default-select-option" nav="local-climate-charts" link="chartmap-btn-chart-link" >Chart</li>
                  <li tabindex="104" id=chartmap-select-map-link"" rel="map" class="default-select-option" nav="local-climate-maps" link="chartmap-btn-map-link">Map</li>
                </ul>
                </ul>
              </div>
            </div>

            <div id="time-wrapper" class="padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div tabindex="105" id="btn-chart" class="btn-annual btn-default-selected circle-box-left d-flex-center width-50" val="annual" sel="time-select-vis">
                Annual
              </div>
              <div tabindex="106" id="btn-chart" class="btn-monthly btn-default circle-box-right d-flex-center width-50" val="monthly" sel="time-select-vis" >
                Monthly
              </div>
            </div>

            <div id="time-select-wrapper" class="rounded-choice-box padding-horrizontal-half padding-vertical-half default-btn-height d-flex-center width-20">
              <div class="select time-select">
                <div tabindex="107" id="time-select-vis" class="select-styled" rel="annual">Annual</div>
                <ul class="select-options">
                  <li tabindex="108" rel="annual" class="default-select-option">Annual</li>
                  <li tabindex="109" rel="monthly" class="default-select-option">Monthly</li>
                </ul>
              </div>
            </div>

            <div tabindex="110" id="chart-info-row" class="chart-info-row-btn rounded-choice-box default-btn-height d-flex-row flex-justify padding-bottom-half d-flex-left d-none " >
              <div class="more-info btn-default">
                <span class="fas fa-info more-info more-icon"></span>
                <span class="fas fa-info more-info less-icon d-none"></span>
                <span class="more">About the graph</span>
                <span class="less d-none">About the graph</span>
              </div>
            </div>

          </div>
        </div>
      </div>

      <div id="chart-row" class="padding-horizontal padding-top-half padding-bottom-half d-flex-row flex-justify" >

          <div id="chart-wrap" class="chart-wrap d-flex d-flex-center width-100 height-80">
            <div id="chart-123" class="chart-canvas width-100 height-100" data-chart-ID="123"></div>
            <div id="chart-message" class="chart-nodata-message d-none"></div>
          </div>
          <div id="annual-slider-wrapper" class="slider-wrap padding-default width-100 d-flex-center d-none">
            <div id="slider-value-low" class="width-10 height-100"></div>
            <div id="slider-range" class="width-80 height-25"></div>
            <div id="slider-value-high" class="width-10 height-100"></div>
          </div>

          <div id="legend-wrapper" class="padding-horrizontal-half default-btn-height d-flex-center">
            <div tabindex="111" id="btn-chart" class="btn-histobs d-flex-center width-50" val="histobs" >
              Historical Observed
              <div class="inner-histobs" val="histobs" ></div>
            </div>
            <div tabindex="112" id="btn-chart" class="btn-hismod selected d-flex-center width-50" val="hist_mod"  >
              Historical Modeled
              <div class="inner-hismod" val="hist_mod"></div>
            </div>
            <div tabindex="113" id="btn-chart" class="btn-lower-emissions selected  d-flex-center width-50" val="rcp45"  >
              Lower Emissions
              <div class="inner-lower-emmisions" val="rcp45" ></div>
            </div>
            <div tabindex="40" id="btn-chart" class="btn-higher-emissions selected d-flex-center width-50" val="rcp85"  >
              Higher Emissions
              <div class="inner-higher-emmisions" val="rcp85"></div>
            </div>
          </div>

          <div id="monthly-select-wrapper" class="padding-horrizontal-half padding-vertical-half default-btn-height d-none width-60">
            <div tabindex="114" id="btn-chart" class="btn-default-selected d-flex-center width-33" val="2025" >
              2010-2030 average
            </div>
            <div tabindex="115" id="btn-chart" class="btn-default d-flex-center width-33" val="2050"  >
              2035-2065 average
            </div>
            <div tabindex="116" id="btn-chart" class="btn-default d-flex-center width-33" val="2075"  >
              2060-2090 average
            </div>
          </div>
      </div>

      <div tabindex="117" id="chart-info-row" class="d-flex-row flex-justify padding-bottom-half d-flex-left d-none" >
        {{> chart-explainer }}
      </div>

      </div>
    </div>

{{> nav_footer }}
{{> footer }}

<script src="https://unpkg.com/terraformer@1.0.8/terraformer.js" integrity="sha384-+M797Pj3WZVCwMmLbOxAoaWYcKJo8NSxItmI48ytcLNeAnfn1d/IckFn31jEqrzP"
        crossorigin="anonymous"></script>
<script src="https://unpkg.com/terraformer-arcgis-parser@1.0.5/terraformer-arcgis-parser.js"
        integrity="sha384-duFUjKTSNoxEspdJNwr83CUgRxclf0ueKJB9DU/Vbit6bfWgzvZsHW6H1JLBBXhp" crossorigin="anonymous"></script>
<script type="text/javascript" src="/resources/js/ce3-ui-components.js"></script>
<script type="text/javascript" src="/resources/js/local-climate-charts.js"></script>
<script type="text/javascript" src="/resources/js/nouislider.min.js"></script>
<script type="text/javascript" src="/resources/js/secondary_header.js"></script>

</body>
</html>
